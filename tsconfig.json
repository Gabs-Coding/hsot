{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "types": ["fvtt-types"]
  },
  "include": ["src/**/*"]
}
